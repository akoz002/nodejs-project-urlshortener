{
	"name": "akoz002-project-urlshortener",
	"version": "0.0.1",
	"description": "freeCodeCamp URL Shortener Microservice project",
	"main": "server.js",
	"scripts": {
		"start": "node --unhandled-rejections=strict server.js",
		"test": "node --unhandled-rejections=strict tests/tests.js",
		"build": "rm -fr public && parcel build views/src/index.html -d public --public-url '/public'",
		"parcel": "parcel watch views/src/index.html -d public --public-url '/public'"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"mongodb": "^3.6.2",
		"mongoose": "^5.10.9"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/akoz002/nodejs-project-urlshortener"
	},
	"keywords": [
		"node",
		"mongoose",
		"express"
	],
	"license": "MIT",
	"devDependencies": {
		"axios": "^0.19.2",
		"parcel-bundler": "^1.12.4",
		"querystring": "^0.2.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-redux": "^7.2.2",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0"
	}
}
